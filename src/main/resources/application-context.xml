<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="country_ru" class="ru.innopolis.stc16.entity.Country">
        <property name="code" value="RU"/>
        <property name="name" value="Russian Federation"/>
    </bean>

    <bean id="person" class="ru.innopolis.stc16.entity.Person" scope="prototype">
        <property name="id" value="-1"/>
        <property name="name" value="Petrov Petr"/>
        <property name="country" ref="country_ru"/>
        <property name="contacts">
            <list>
                <bean class="ru.innopolis.stc16.entity.Contact">
                    <constructor-arg value="+79110000000"/>
                </bean>
            </list>
        </property>
    </bean>

</beans>